{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "settings": {
      "jsx": true
    },
    "rules": {
      "no-named-as-default": 0,
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "react/destructuring-assignment": 0,
      "no-restricted-globals": 0
    },
    "globals": {
      "fetch": false
    }
}